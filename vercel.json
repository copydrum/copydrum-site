{
  "rewrites": [
    {
      "source": "/:lang(ko|ja|zh-cn|zh-tw|de|fr|es|vi|th|hi|id|pt|ru|it|tr|uk)/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.copydrum.com"
        }
      ],
      "destination": "https://copydrum.com",
      "permanent": true
    }
  ]
}

