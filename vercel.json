{
  "rewrites": [
    {
      "source": "/:lang(ko|ja|zh-cn|zh-tw|de|fr|es|vi|th|hi|id|pt|ru|it|tr|uk)/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    { "src": "/sheet-detail/(.*)", "dest": "/sheet-detail/$1", "prerender": true },
    { "src": "/categories/(.*)", "dest": "/categories/$1", "prerender": true },
    { "src": "/categories", "dest": "/categories", "prerender": true },
    { "src": "/", "dest": "/", "prerender": true }
  ]
}